                "properties": {
                    "template": "BondPricing",
                    "tenor": "UST_10Y"
                }
            },
            {
                "type": "TEMPLATE",
                "properties": {
                    "template": "BondPricing",
                    "tenor": "UST_30Y"
                }
            },
            {
                "name": "Global.Score",
                "type": "AVERAGE",
                "inputs": {
                    "i0": "UST_2Y.mid",
                    "i1": "UST_3Y.mid",
                    "i2": "UST_5Y.mid",
                    "i3": "UST_10Y.mid",
                    "i4": "UST_30Y.mid"
                }
