{
    "graph": {
        "name": "BOND PRICER",
        "version": "1.0",
        "templates": [
            {
                "name": "Venue",
                "nodes": [
                    {
                        "name": "{{prefix}}.bid",
                        "type": "SCALAR_SOURCE"
                    },
                    {
                        "name": "{{prefix}}.bidQty",
                        "type": "SCALAR_SOURCE"
                    },
                    {
                        "name": "{{prefix}}.ask",
                        "type": "SCALAR_SOURCE"
                    },
                    {
                        "name": "{{prefix}}.askQty",
                        "type": "SCALAR_SOURCE"
                    }
                ]
            },
            {
                "name": "BondPricing",
                "nodes": [
                    {
                        "type": "TEMPLATE",
                        "properties": {
                            "template": "Venue",
                            "prefix": "{{tenor}}.Btec"
                        }
                    },
                    {
                        "type": "TEMPLATE",
                        "properties": {
                            "template": "Venue",
                            "prefix": "{{tenor}}.Fenics"
                        }
                    },
                    {
                        "type": "TEMPLATE",
                        "properties": {
                            "template": "Venue",
                            "prefix": "{{tenor}}.Dweb"
                        }
                    },
                    {
                        "name": "{{tenor}}.wBid",
                        "type": "WEIGHTED_AVG",
                        "inputs": {
                            "i0": "{{tenor}}.Btec.bid",
                            "i1": "{{tenor}}.Btec.bidQty",
                            "i2": "{{tenor}}.Fenics.bid",
                            "i3": "{{tenor}}.Fenics.bidQty",
                            "i4": "{{tenor}}.Dweb.bid",
                            "i5": "{{tenor}}.Dweb.bidQty"
                        }
                    },
                    {
                        "name": "{{tenor}}.wAsk",
                        "type": "WEIGHTED_AVG",
                        "inputs": {
                            "i0": "{{tenor}}.Btec.ask",
                            "i1": "{{tenor}}.Btec.askQty",
                            "i2": "{{tenor}}.Fenics.ask",
                            "i3": "{{tenor}}.Fenics.askQty",
                            "i4": "{{tenor}}.Dweb.ask",
                            "i5": "{{tenor}}.Dweb.askQty"
                        }
                    },
                    {
                        "name": "{{tenor}}.mid",
                        "type": "AVERAGE",
                        "inputs": {
                            "i0": "{{tenor}}.wBid",
                            "i1": "{{tenor}}.wAsk"
                        }
                    }
                ]
            }
        ],
        "nodes": [
            {
                "type": "TEMPLATE",
                "properties": {
                    "template": "BondPricing",
                    "tenor": "UST_2Y"
                }
            },
            {
                "type": "TEMPLATE",
                "properties": {
